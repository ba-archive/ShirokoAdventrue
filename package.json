{
  "name": "shiroko-adventure",
  "version": "3.0.0",
  "description": "A Vue.js project",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --config .prettierrc.json -uw ./src/",
    "prepare": "husky install",
